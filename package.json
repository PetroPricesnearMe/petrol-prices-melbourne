{
  "name": "melbourne-petrol-stations",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.3.4",
    "framer-motion": "^10.0.1",
    "mapbox-gl": "^3.14.0",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-map-gl": "^8.0.4",
    "react-router-dom": "^6.8.0",
    "react-scripts": "^5.0.1",
    "socket.io-client": "^4.6.1",
    "styled-components": "^5.3.9",
    "web-vitals": "^3.5.0"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "build:analyze": "ANALYZE=true craco build",
    "test": "craco test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "analyze": "npm run build && npx serve -s build",
    "analyze-bundle": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "lighthouse": "npm run build && npx lighthouse http://localhost:3000 --output-path=./lighthouse-report.html --view",
    "lighthouse:ci": "npm run build && npx lighthouse-ci autorun",
    "security-audit": "npm audit --audit-level=moderate",
    "security-audit:fix": "npm audit fix",
    "performance-test": "npm run build && npx lighthouse http://localhost:3000 --only-categories=performance --output=json --output-path=./performance-report.json",
    "clean": "rm -rf build node_modules/.cache",
    "clean:all": "rm -rf build node_modules && npm install",
    "optimize-images": "node scripts/optimize-images.js",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "size": "npm run build && size-limit"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@types/leaflet": "^1.9.3",
    "babel-plugin-transform-imports": "^2.0.0",
    "brotli-webpack-plugin": "^1.1.0",
    "compression-webpack-plugin": "^11.1.0",
    "imagemin": "^9.0.1",
    "imagemin-avif": "^0.1.6",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-webp": "^8.0.0",
    "sharp": "^0.34.4",
    "terser-webpack-plugin": "^5.3.14",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
